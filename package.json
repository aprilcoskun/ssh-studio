    "electron-store": "6.0.0",
    "jquery": "^3.5.1",
    "ssh2": "^0.8.9",
    "xterm": "4.8.1",
    "xterm-addon-fit": "^0.4.0",
    "xterm-addon-web-links": "^0.4.0",
    "xterm-addon-webgl": "^0.8.0"
  },
  "build": {
    "appId": "com.nisancoskun.ssh-studio",
    "productName": "SSH Studio",
    "copyright": "Copyright © 2020 Nisan Coşkun",
    "linux": {
      "category": "Network"
    },
    "mac": {
