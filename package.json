{
  "name": "ssh-studio",
  "version": "1.7.3",
  "description": "SSH & Telnet Client",
  "homepage": "https://github.com/aprilcoskun/ssh-studio",
  "author": {
    "name": "Nisan Coşkun",
    "email": "info@nisancoskun.com"
  },
  "main": "src/index.js",
  "license": "MIT",
  "scripts": {
    "start": "npm run electron:serve",
    "electron:serve": "electron . --serve",
